{
  "name": "startown",
  "version": "0.1.18",
  "description": "Home to a collection of superstar atomic react components",
  "main": "lib/index.js",
  "repository": "https://github.com/Brianzchen/startown.git",
  "author": "Brian Chen",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib",
    "build:flow": "flow-copy-source --ignore '*/**/*.{stories,spec}.*' src lib",
    "build": "NODE_ENV=production babel src --out-dir lib && yarn build:flow && cp src/types.js types.js",
    "prepublishOnly": "yarn clean && yarn jest && yarn flow && yarn build",
    "docgen": "react-docgen src --out=website/components.json --pretty --exclude='/.*[^(index\\.js$)]/' && node ./scripts/post-docgen.js",
    "website:prep": "rm -rf website/dist && yarn docgen && node ./scripts/getStoriesList.js",
    "website:build": "yarn website:prep && MODE=website NODE_ENV=production yarn webpack --config website/webpack.config.js",
    "website:start": "yarn website:prep && yarn webpack serve --config website/webpack.config.js"
  },
  "dependencies": {
    "aphrodite": "^2.4.0",
    "color": "^4.2.1",
    "deepmerge": "^4.2.2",
    "hex-to-rgb": "^1.0.1",
    "key-commander": "^0.1.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-syntax-flow": "^7.16.7",
    "@babel/plugin-syntax-jsx": "^7.16.7",
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-flow": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@faker-js/faker": "^5.5.3",
    "@testing-library/react": "^12.1.3",
    "app-root-path": "^3.0.0",
    "axios": "^0.26.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-flow-enums": "^0.0.2",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-bzc": "^1.0.9",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-fb-flow": "^0.0.4",
    "eslint-plugin-ft-flow": "^2.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.0.5",
    "flow-bin": "^0.202.0",
    "flow-copy-source": "^2.0.9",
    "flow-enums-runtime": "^0.0.6",
    "jest": "^27.5.1",
    "react": "17.0.0",
    "react-docgen": "^5.4.0",
    "react-dom": "17.0.0",
    "react-media": "^1.10.0",
    "react-router-dom": "^6.2.1",
    "react-syntax-highlighter": "^15.4.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
